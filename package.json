{
  "name": "vue-tiny-virtual-list",
  "version": "1.3.0",
  "description": "Tiny(3kb) & Virtual scroll list & Huge amount data & High performance (support vue2.x&vue3.x)",
  "author": "keno-lee",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "vite example --host -c ./scripts/dev.ts --port 2222",
    "build:docs": "vite build -c ./scripts/build.docs.ts",
    "build": "vite build -c ./scripts/build.ts && tsc -p tsconfig.build.json",
    "lint": "eslint ./example --ext .vue,.ts,.tsx,.js,.jsx --fix"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@rushstack/eslint-patch": "^1.1.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitejs/plugin-vue2-jsx": "^1.0.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^10.3.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "sass": "1.63.6",
    "start-server-and-test": "^1.14.0",
    "terser": "^5.14.2",
    "typescript": "~5.2.0",
    "vite": "^3.0.2",
    "vitest": "^0.18.1",
    "vue": "^2.7.7",
    "vue-router": "^3.5.4",
    "vue-template-compiler": "^2.7.7",
    "vue-tsc": "^1.8.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keno-lee/vue-tiny-virtual-list.git"
  },
  "bugs": {
    "url": "https://github.com/keno-lee/vue-tiny-virtual-list/issues"
  },
  "homepage": "https://github.com/keno-lee/vue-tiny-virtual-list#readme",
  "directories": {
    "doc": "docs",
    "example": "example",
    "lib": "lib"
  }
}
