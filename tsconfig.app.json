{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "example/**/*", "example/**/*.vue", "src/*.js"],
  "exclude": ["example/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@/example/*": ["./example/*"],
      "@/src/*": ["./src/*"]
    }
  }
}
