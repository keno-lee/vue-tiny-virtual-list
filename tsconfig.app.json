{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  // "include": ["env.d.ts", "example/**/*", "example/**/*.vue"],
  // "exclude": ["example/**/__tests__/*"],
  "include": ["src", "example"],
  "exclude": ["node_modules", "lib"],
  "compilerOptions": {
    "composite": true,
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@/example/*": ["./example/*"],
      "@/src/*": ["./src/*"]
    }
  }
}
